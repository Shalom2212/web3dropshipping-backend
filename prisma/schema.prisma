
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SupplierData {
  id Int @default(autoincrement()) @id
  productName String
  productPrice String
  supplierName String
  supplierWalletAddress String
  maxContractPeriod Int
  supplierPercentage Int
  totalNumberOfProduct Int
}
